{
  "compilerOptions": {
    "jsx": "react-jsx", 
    "module": "esnext",
    "target": "es6",
    "allowJs": true,
    "checkJs": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./src",
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
  },
  "include": ["src"]
}